{
	"extends": ["next/core-web-vitals", "prettier"],
	"plugins": ["unused-imports", "@typescript-eslint"],
	"rules": {
		"eqeqeq": 1,
		"no-unused-vars": "off",
		"@typescript-eslint/no-unused-vars": [
			"error",
			{
				"vars": "all",
				"args": "all",
				"ignoreRestSiblings": true,
				"argsIgnorePattern": "^_",
				"caughtErrorsIgnorePattern": "^_"
			}
		]
	}
}
